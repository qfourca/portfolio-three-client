{
	"name": "yeormae",
	"scripts": {
		"prepare": "husky install",
		"lint": "eslint packages/",
		"format-fix": "prettier \"**/*.{ts,tsx,js,jsx}\" --write",
		"format-check": "prettier --check \"**/*.{ts,tsx,js,jsx}\"",
		"lint-cache": "eslint packages/ --cache",
		"lint-fix": "eslint packages/ --cache --fix",
		"start": "vite serve --port 3000",
		"start:host": "vite serve --port 3000 --host 0.0.0.0",
		"dev": "vite serve --port 3000",
		"build": "rm -rf dist && vite build",
		"build:dev": "rm -rf dist && vite build --mode development",
		"build:typecheck": "tsc --noEmit && rm -rf dist && vite.config.ts && npm run remove:map",
		"preview": "vite preview"
	},
	"engines": {
		"npm": ">=8.x <=10.x",
		"node": ">=14.x <=20.x"
	},
	"author": "Geonho Kim",
	"version": "0.0.0",
	"license": "ISC",
	"private": true,
	"devDependencies": {
		"@commitlint/cli": "^12.1.4",
		"@commitlint/config-conventional": "^12.1.4",
		"@swc/core": "^1.3.94",
		"@swc/jest": "^0.2.10",
		"@types/eslint": "^8.2.2",
		"@types/graphlib": "^2.1.7",
		"@types/i18n-js": "^3.0.1",
		"@types/jest": "^29.2.3",
		"@types/node": "^16.3",
		"@types/react": "^18.0.25",
		"@types/react-color": "^3.0.1",
		"@types/react-dom": "^18.0.8",
		"@types/react-redux": "^7.1.1",
		"@types/styled-components": "^5.1.26",
		"@types/three": "^0.148.1",
		"babel-plugin-import": "^1.13.5",
		"browserslist-to-esbuild": "^1.2.0",
		"css-loader": "^6.5.1",
		"eslint": "^8.6.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-typescript": "^16.1.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-atomic-design": "^1.0.1",
		"eslint-plugin-file-progress": "^1.2.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.28.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"file-loader": "^6.2.0",
		"husky": "^7.0",
		"lint-staged": "^13.0.3",
		"prettier": "^2.5.1",
		"rollup-plugin-node-builtins": "^2.1.2",
		"simple-git": "^3.22.0",
		"style-loader": "^3.3.1",
		"ts-node": "^10.1",
		"tsconfig-paths-webpack-plugin": "^4.0.0",
		"typescript": "~4.5",
		"url-loader": "^4.1.1",
		"vite": "^4.2.0",
		"vite-plugin-babel-macros": "^1.0.6",
		"vite-plugin-imp": "^2.3.1",
		"vite-plugin-svgr": "^4.2.0"
	},
	"dependencies": {
		"@react-three/drei": "^9.96.1",
		"@react-three/fiber": "^8.15.14",
		"@tweenjs/tween.js": "^23.1.1",
		"@vitejs/plugin-react": "^4.2.1",
		"axios": "^1.1.3",
		"chalk": "^4.1.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"recoil": "^0.7.7",
		"styled-components": "^5.3.6",
		"three": "^0.160.1",
		"typesafe-actions": "^5.1.0"
